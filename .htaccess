<IfModule mod_rewrite.c>
    RewriteEngine On

    # Remove .php extension from URLs
    RewriteCond %{REQUEST_FILENAME}.php -f
    RewriteRule ^([^/]+)$ $1.php [L]

    # Redirect to index.php when accessing the root of your project
    RewriteRule ^$ /TuteeFind/index.php [L]
    
    # Redirect everything else to index.php (optional, depending on your routing logic)
    RewriteRule ^(.*)$ /TuteeFind/index.php [L]
</IfModule>
